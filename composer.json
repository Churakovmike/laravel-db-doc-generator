{
    "name": "churakovmike/laravel-db-doc-generator",
    "type": "library",
    "description": "Database documentation generator for laravel",
    "keywords": [
        "laravel",
        "database documentation",
        "laravel documentation"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Churakov Mike"
        }
    ],
    "require": {
        "ext-tokenizer": "*",
        "doctrine/dbal": "2.9.x-dev",
        "illuminate/console": "^5.5|^6|^7",
        "illuminate/contracts": "^5.5|^6|^7",
        "illuminate/database": "^5.5|^6|^7",
        "illuminate/http": "^5.5|^6|^7",
        "illuminate/pagination": "^5.5|^6|^7",
        "illuminate/support": "^5.5|^6|^7",
        "illuminate/view": "^5.5|^6|^7"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\DbDocumentor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChurakovMike\\DbDocumentor\\DbDocumentorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
